{
  "Logging": {
    "Console": {
      "DisableColors": true
    },
    "IncludeCategory": false,
    "IncludeEventId": false,
    "IncludeException": true,
    "IncludeLogLevel": true,
    "IncludeNewline": true,
    "IncludeScopes": true,
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Information",
      "AWSSDK": "Warning"
    }
  },
  "DynamoDB": {
    "UseHttp": true
  },
  "QueueUrl": "https://sqs.<region>.amazonaws.com/<account>/CallableMessagingQueue"
}
